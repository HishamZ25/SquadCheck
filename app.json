{
  "expo": {
    "name": "SquadCheck",
    "slug": "squadcheck",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FF6B35"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.squadcheck.app"
    },
    "android": {
      "package": "com.squadcheck.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-image-picker",
      "expo-camera",
      "expo-media-library"
    ],
    "permissions": [
      "CAMERA",
      "MEDIA_LIBRARY",
      "NOTIFICATIONS"
    ]
  }
} 